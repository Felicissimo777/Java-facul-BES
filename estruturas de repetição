package repetiçãocambalhota;
import java.util.Scanner;

public class Cambalhota {

	public static void main(String[] args) {
		Scanner Teclado = new Scanner(System.in);
		int cc = 10;
		while(cc <= 4) {
			System.out.println("Cambalhota");
			cc++;
		}
		for(int i = cc; i <= 4; i++) {
			System.out.println("Cambalhota");
			cc++;

	}
		

}

}
